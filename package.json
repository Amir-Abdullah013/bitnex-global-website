{
  "name": "bitnex-global",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:ws": "node websocket-server.js",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:ws\"",
    "build": "next build --turbopack",
    "start": "next start",
    "analyze": "ANALYZE=true npm run build",
    "optimize": "node scripts/optimize.js",
    "lighthouse": "lighthouse http://localhost:3000 --output html --output-path ./lighthouse-report.html",
    "test:performance": "npm run build && npm run start & sleep 5 && npm run lighthouse && pkill -f 'next start'",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node prisma/seed.js",
    "seed:trading-pairs": "node scripts/seed-trading-pairs.js",
    "seed:price-data": "node scripts/seed-price-data.js",
    "seed:fee-structures": "node scripts/seed-fee-structures.js",
    "seed:portfolio-data": "node scripts/seed-portfolio-data.js",
    "migrate:check": "node scripts/migrate-to-supabase.js",
    "env:check": "node scripts/update-env-vars.js",
    "env:migration-check": "node scripts/check-env-migration.js",
    "fix-imports": "node scripts/fix-all-remaining-imports.js",
    "setup:env": "node setup-helper.js",
    "env:setup": "node scripts/setup-env-variables.js"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "jsonwebtoken": "^9.0.2",
    "lightweight-charts": "^4.1.3",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "node-fetch": "^3.3.2",
    "nodemailer": "^7.0.9",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "recharts": "^2.15.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.2.1",
    "glob": "^11.0.3",
    "lighthouse": "^12.0.0",
    "postcss": "^8.4.0",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
